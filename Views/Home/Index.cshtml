@{
    ViewBag.Title = "Home Page";
}


<div class="row">
    <p>Claims if user is signed-in...</p>


    <table>
        <tr><th>Type</th><th>Value</th></tr>
        @foreach (var c in ViewBag.Claims)
        {
            <tr><td>@c.Type</td><td>@c.Value</td></tr>
        }
    </table>
</div>