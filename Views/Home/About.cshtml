@{
    ViewBag.Title = "About";
    
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<p>Use this area to provide additional information.</p>


<table>
    <tr><th>Type</th><th>Value</th></tr>
@foreach(var c in ViewBag.Claims)
{
    <tr><td>@c.Type</td><td>@c.Value</td></tr>
}
</table>

<div>
@ViewBag.UserJson
</div>



